  <thead>
    <tr>
      <th>ID</th>
      <th>Date</th>
      <th>Grade</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each climbs as |climb|}}
    <tr data-id="{{climb.id}}">
      <th scope="row">{{climb.id}}</th>
      <td>{{climb.date}}</td>
      <td>{{climb.grade}}</td>
      <td><span class="glyphicon glyphicon-edit" aria-hidden="true"></span></td>
      <td><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></td>
    </tr>
    {{/each}}
  </tbody>
