<thead>
  <tr>
    <th>ID</th>
    <th class="text-center">Date</th>
    <th class="text-center">Grade</th>
    <th class="text-center">Edit</th>
    <th class="text-center">Delete</th>
  </tr>
</thead>
<tbody>
  {{#each climbs as |climb|}}
    <tr data-id="{{climb.id}}">
      <th scope="row">{{climb.id}}</th>
      <td>{{climb.date}}</td>
      <td>{{climb.grade}}</td>
      <td><button id="update-climb-primary-button" type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#updateClimbModal"><span class="glyphicon glyphicon-edit" aria-hidden="true"></span></button></td>
      <td><button type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#destroyClimbModal"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></button>
        <div id="destroyClimbModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="destroyClimbModalLabel">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="destroyClimbModalLabel">Are you sure?</h4>
              </div>
              <div class="handlebar-id-placer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Nope</button>
                <input data-id="{{climb.id}}" type="submit" class="btn trash btn-primary" name="submit" value="Yaaaasss"><br><br>
              </div>
            </div>
          </div>
        </div>
      </td>

    </tr>
  {{/each}}
</tbody>
